{
  "firestore": {
    "rules": "frontend_disabled/firestore.rules"
  },
  "hosting": [
    {
      "target": "cbtpromax",
      "public": "frontend_disabled/build",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ],
      "headers": [
        {
          "source": "**/*.@(js|css)",
          "headers": [
            {
              "key": "Cache-Control",
              "value": "max-age=31536000"
            }
          ]
        }
      ]
    },
    {
