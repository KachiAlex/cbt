{
  "version": 2,
  "name": "cbt-backend",
  "builds": [
    {
      "src": "src/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "JWT_SECRET": "@jwt_secret"
  },
  "functions": {
    "src/server.js": {
      "maxDuration": 30
    }
  },
  "type": "nodejs"
} 