# Firebase Configuration
# Get these values from your Firebase project settings

REACT_APP_FIREBASE_API_KEY=your-api-key-here
REACT_APP_FIREBASE_AUTH_DOMAIN=your-project-id.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=your-project-id
REACT_APP_FIREBASE_STORAGE_BUCKET=your-project-id.appspot.com
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=your-sender-id
REACT_APP_FIREBASE_APP_ID=your-app-id
REACT_APP_FIREBASE_MEASUREMENT_ID=your-measurement-id

# Optional: Firebase Functions URL (if using cloud functions)
REACT_APP_FIREBASE_FUNCTIONS_URL=https://your-region-your-project-id.cloudfunctions.net

# App Configuration
REACT_APP_APP_NAME=African Traditional Rulers
REACT_APP_APP_VERSION=1.0.0
REACT_APP_ENVIRONMENT=development

# Optional: Analytics and Performance
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_PERFORMANCE=true
